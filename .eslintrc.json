{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:all", "plugin:react/all"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "sort-imports": "off",
    "sort-vars": "off",
    "react/jsx-indent": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-no-literals": "off",
    "react/jsx-newline": "off"
  }
}
